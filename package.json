{
  "name": "nft-mint-pro",
  "version": "1.0.0",
  "description": "Professional NFT Minting & Smart Contract Automation Tool",
  "main": "electron/main.js",
  "author": "Your Name",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "react-start": "cross-env BROWSER=none webpack serve --mode development",
    "electron-start": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "webpack --mode production",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "test": "jest",
    "lint": "eslint src/ electron/",
    "format": "prettier --write \"src/**/*.{js,jsx}\" \"electron/**/*.js\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "immer": "^10.0.3",
    "lowdb": "^7.0.0",
    "lucide-react": "^0.263.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "viem": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "build": {
    "appId": "com.nftmintpro.app",
    "productName": "NFT Mint Pro",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    }
  }
}
